<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="anchorPane" prefHeight="800.0" prefWidth="600.0" style="-fx-background-color: #000000;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="p4_group_8_repo.Controllers.MainMenuController">

    <children>
        <ImageView fitHeight="800.0" fitWidth="600.0" layoutX="1.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../Assets/menu_background.png" />
            </image>
        </ImageView>
        <StackPane layoutX="219.0" layoutY="470.0" prefHeight="277.0" prefWidth="164.0" AnchorPane.topAnchor="470.0">
            <children>
                <Button fx:id="Play" mnemonicParsing="false" onAction="#playGame" prefHeight="20.0" prefWidth="180.0" text="PLAY" translateY="-110.0">
                    <font>
                        <Font size="20.0" />
                    </font>
                </Button>
                <Button fx:id="Highscore" layoutX="10.0" layoutY="123.0" mnemonicParsing="false" onAction="#highScore" prefHeight="20.0" prefWidth="180.0" text="HIGHSCORE" translateY="-50.0">
                    <font>
                        <Font size="20.0" />
                    </font>
                </Button>
                <Button fx:id="Help" layoutX="10.0" layoutY="123.0" mnemonicParsing="false" onAction="#instructions" prefHeight="20.0" prefWidth="180.0" text="HELP" translateY="10.0">
                    <font>
                        <Font size="20.0" />
                    </font>
                </Button>
                <Button fx:id="Exit" layoutX="10.0" layoutY="123.0" mnemonicParsing="false" onAction="#exitGame" prefHeight="20.0" prefWidth="180.0" text="EXIT" translateY="70.0">
                    <font>
                        <Font size="20.0" />
                    </font>
                </Button>
            </children></StackPane>
    </children>
</AnchorPane>
